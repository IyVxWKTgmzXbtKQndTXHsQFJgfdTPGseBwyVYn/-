Submit.MouseButton1Click:Connect(
    function()
        if TextBox.Text == getgenv().NightHubKeySystemSettings["Correct Key"] then
            NightHub:Destroy()
            MainFrame.Visible = false
            loadstring(game:HttpGet(getgenv().NightHubKeySystemSettings["Script Link"]))()
        else
            print("Incorrect Key!")
        end
    end
)

